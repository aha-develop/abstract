{
  "name": "@aha-develop/abstract",
  "description": "Abstract integration",
  "version": "1.0.0",
  "author": "support@aha.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/aha-develop/abstract"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.8.0"
  },
  "ahaExtension": {
    "cspSources": [
      "https://app.goabstract.com",
      "https://app.abstract.com"
    ],
    "contributes": {
      "views": {
        "abstractAttribute": {
          "title": "Abstract",
          "entryPoint": "src/views/attribute.js",
          "host": "attribute",
          "recordTypes": [
            "Feature",
            "Requirement",
            "Epic"
          ]
        },
        "abstractPanel": {
          "title": "Abstract",
          "entryPoint": "src/views/panel.js",
          "host": "panel"
        }
      }
    }
  },
  "dependencies": {
    "@aha-develop/aha-develop-react": "^1.2.1"
  }
}
